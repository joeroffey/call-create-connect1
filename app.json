{
  "expo": {
    "name": "EezyBuild",
    "slug": "eezybuild",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/favicon.ico",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/favicon.ico",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eezybuild.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/favicon.ico",
        "backgroundColor": "#000000"
      },
      "package": "com.eezybuild.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./public/favicon.ico",
      "bundler": "metro"
    },
    "main": "index.js"
  }
}